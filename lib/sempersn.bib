@article{haefner2018MLCSESPRIT,
  author    = {Haefner, Stephan and Thom\"a, Reiner},
  title     = {High Resolution Estimation of {AoA,} {AoD} and {TdoA} from {MIMO} Channel Sounding Measurements with Virtual Antenna Arrays: {Maximum-likelihood} vs. Unitary Tensor-\{ESPRIT\}},
  journal   = {International Journal of Advances in Telecommunications, Electrotechnics, Signals and Systems},
  volume    = {7},
  number    = {2},
  date      = {2018},
  keywords  = {},
  abstract  = {Estimating the parameters of a geometric propagation model from MIMO channel sounding measurements will be considered, which requires the solution of an inverse problem. Thus, a model of the measured data is derived, which incorporates a model of the measurement system as well as the parameters of interest. Based on the data model a maximum-likelihood estimator will be derived to infer the model parameters. Because virtual antenna arrays are considerer, formed by step-wise rotating directive antennas at transmitter and receiver side, the MIMO measurements are conducted in the beam-space. Hence, the data model can be described by a multidimensional convolution of the measurement system and the propagation channel. Based on the convolutional modelling, the parameter estimation problem is transformed into a harmonic retrieval problem, which can be solved by an Unitary Tensor-ESPRIT algorithm. The maximum-likelihood and ESPRIT estimator are compared by Monte-Carlo simulations according to their root-mean-square estimation error.},
  issn      = {1805-5443},
  pages     = {27},
  doi       = {10.11601/ijates.v7i2.254},
  url       = {https://doi.org/10.11601/ijates.v7i2.254},
  source    = {Crossref},
  publisher = {International Science and Engineering Society},
  year      = {2018},
  month     = jun
}

