@article{tropp2009cosamp,
 author = {Needell, D. and Tropp, J.A.},
 title = {{CoSaMP:} {Iterative} signal recovery from incomplete and inaccurate samples},
 journal = {Appl. Comput. Harmon. A.},
 volume = {26},
 number = {3},
 pages = {301--321},
 date = {2009},
 issn = {1063-5203},
 doi = {10.1016/j.acha.2008.07.002},
 url = {https://doi.org/10.1016/j.acha.2008.07.002},
 keywords = {Algorithms, Approximation, Basis pursuit, Compressed sensing, Orthogonal matching pursuit, Restricted isometry property, Signal recovery, Sparse approximation, Uncertainty principle},
 abstract = {Compressive sampling offers a new paradigm for acquiring signals that are compressible with respect to an orthonormal basis. The major algorithmic challenge in compressive sampling is to approximate a compressible signal from noisy samples. This paper describes a new iterative recovery algorithm called CoSaMP that delivers the same guarantees as the best optimization-based approaches. Moreover, this algorithm offers rigorous bounds on computational cost and storage. It is likely to be extremely efficient for practical problems because it requires only matrix{\textendash}vector multiplies with the sampling matrix. For compressible signals, the running time is just O(Nlog2N), where N is the length of the signal.},
 source = {Crossref},
 publisher = {Elsevier BV},
 year = {2009},
 month = may,
}

