 doi = {10.1073/pnas.0437847100},
 publisher = {Proceedings of the National Academy of Sciences},
 abstract = {Given a dictionary D = \{{\textbackslash}textbackslash\}{\textbackslash}\{dk\{{\textbackslash}textbackslash\}{\textbackslash}\} of vectors dk, we seek to represent a signal S as a linear combination S = \{{\textbackslash}textbackslash\}ensuremath{\textbackslash}\{\{{\textbackslash}textbackslash\}sum{\textbackslash}\}k \{{\textbackslash}textbackslash\}ensuremath{\textbackslash}\{\{{\textbackslash}textbackslash\}gamma{\textbackslash}\}(k)dk, with scalar coefficients \{{\textbackslash}textbackslash\}ensuremath{\textbackslash}\{\{{\textbackslash}textbackslash\}gamma{\textbackslash}\}(k). In particular, we aim for the sparsest representation possible. In general, this requires a combinatorial optimization process. Previous work considered the special case where D is an overcomplete system consisting of exactly two orthobases and has shown that, under a condition of mutual incoherence of the two bases, and assuming that S has a sufficiently sparse representation, this representation is unique and can be found by solving a convex optimization problem: specifically, minimizing the l1 norm of the coefficients \{{\textbackslash}textbackslash\}ensuremath{\textbackslash}\{\{{\textbackslash}textbackslash\}gamma{\textbackslash}\}̱. In this article, we obtain parallel results in a more general setting, where the dictionary D can arise from two or several bases, frames, or even less structured systems. We sketch three applications: separating linear features from planar ones in 3D data, noncooperative multiuser encoding, and identification of over-complete independent component models.},
